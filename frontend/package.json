	"private": true,
	"type": "module",
	"description": "A showcase demo application inspired by Star Wars fictional universe",
	"workspaces": [
		"./src/types",
		"./src/utils",
		"./src/infra/http/*",
		"./src/components/vue-js",
		"./src/features/*",
		"./src/apps/vue-js"
	],
	"scripts": {
		"start:mocks": "node __mocks__/index.mjs",
		"build-only": "npm run build-only --workspaces --if-present",
		"format:check": "npm run format:check --workspaces --if-present",
		"format:fix": "npm run format:fix --workspaces --if-present",
		"lint:check": "npm run lint:check --workspaces --if-present",
		"test:unit": "npm run test:unit --workspaces --if-present",
		"test:unit:coverage": "npm run test:unit --workspaces --if-present -- --coverage",
		"type:check": "npm run type:check --workspaces --if-present"
	},
	"devDependencies": {
		"@amjs/js-utils": "^0.5.0",
		"@fortawesome/fontawesome-svg-core": "^6.7.2",
