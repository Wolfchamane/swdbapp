		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@tsconfig/node22": "^22.0.0",
		"@types/jsdom": "^21.1.6",
		"@types/node": "^20.14.2",
		"@vitest/coverage-istanbul": "^3.0.8",
		"@vitest/eslint-plugin": "^1.1.36",
		"browserslist": "^4.23.0",
		"eslint": "^9.0.0",
		"globals": "^16.0.0",
		"jsdom": "^24.1.0",
